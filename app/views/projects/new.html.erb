<h1>Create a Project.</h1>

<%= form_for @project do |f| %>
  <div class="field">
    <%= f.label :title %> </br>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :description %> </br>
    <%= f.text_area :description %>
  </div>

  <div class="field">
    <%= f.label :goal %> </br>
    <%= f.number_field :goal %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>

  
<% end %>
